{
  "name": "invm",
  "version": "0.1.0",
  "description": "INVM - A node.js inventory manager",
  "main": "index.js",
  "scripts": {
    "test": "npm run build-debug && node build/index.js -p 3 --ppp 4",
    "build-debug": "tsc --project tsconfig.json",
    "build": "npm run lint && tsc --project tsconfig.production.json",
    "lint": "eslint src/**.*",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkohler616/invm.git"
  },
  "keywords": [
    "inventory",
    "manager",
    "json"
  ],
  "author": "Benjamin Kohler",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bkohler616/invm/issues"
  },
  "homepage": "https://github.com/bkohler616/invm#readme",
  "dependencies": {
    "@inquirer/prompts": "3.3.0",
    "log4js": "6.9.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "@types/yargs": "17.0.31",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.0",
    "eslint-plugin-promise": "6.1.1",
    "typescript": "5.2.2"
  }
